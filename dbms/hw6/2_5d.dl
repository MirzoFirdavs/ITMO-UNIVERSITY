WithMark(StudentId, StudentName, GroupName) :- Students(StudentId, StudentName, GroupId), Groups(GroupId, GroupName, _), Marks(StudentId, CourseId, _), Courses(CourseId, :CourseName).
Info(StudentId, StudentName, GroupName) :- Students(StudentId, StudentName, GroupId), Groups(GroupId, GroupName, _), not WithMark(StudentId, StudentName, GroupName).