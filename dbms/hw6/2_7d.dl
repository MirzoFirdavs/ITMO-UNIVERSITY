WithMarks(StudentId, StudentName, GroupName) :- Students(StudentId, StudentName, GroupId), Groups(GroupId, GroupName, _), Courses(CourseId, :CourseName), Marks(StudentId, CourseId, _).
Info(StudentId, StudentName, GroupName) :- Students(StudentId, StudentName, GroupId), Groups(GroupId, GroupName, _), Courses(CourseId, :CourseName), Plan(GroupId, CourseId, _), not WithMarks(StudentId, StudentName, GroupName).